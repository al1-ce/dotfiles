# LINK: https://github.com/felipesaa/A-vim-like-firefox-like-configuration-for-w3m/tree/master

# Ctrl      : C-, ^
# Escape    : ESC-, M-, ^[
# Space     : SPC, ' '
# Tab       : TAB, ^i, ^I
# Shift+Tab : ^[[Z
# Delete    : DEL, ^?
# Up        : UP, ^[[A
# Down      : DOWN, ^[[B
# Right     : RIGHT, ^[[C
# Left      : LEFT, ^[[D
# Re Pag    : M-[5~
# Av Pag    : M-[6~
# Enter     : C-j  RET(pendant confirm)

keymap ESC-v NULL
keymap ESC-< NULL
keymap ESC-> NULL
keymap ESC-m NULL
keymap ESC-C-j NULL
keymap ESC-C-m NULL
keymap ESC-w NULL
keymap ESC-W NULL
keymap ESC-l NULL
keymap ESC-s NULL
keymap ESC-a NULL
keymap ESC-b NULL
keymap ESC-: NULL
keymap ESC-g NULL
keymap ESC-TAB NULL
keymap ESC-I NULL
keymap ESC-C-j NULL
keymap ESC-M NULL
keymap ESC-u NULL
keymap ESC-e NULL
keymap ESC-t NULL
keymap ESC-n NULL
keymap ESC-p NULL
keymap ESC-c NULL
keymap ESC-o NULL
keymap ESC-k NULL

keymap o        GOTO
keymap C-d      NEXT_PAGE
keymap C-u      PREV_PAGE
keymap O        OPTIONS
keymap w        NEXT_WORD
keymap b        PREV_WORD
keymap C-]      GOTO_LINK
keymap L        GOTO_LINK
keymap gg       BEGIN
keymap K        NEXT_LEFT_UP
keymap J        NEXT_RIGHT_DOWN
keymap C-o      MOVE_LIST_MENU
keymap ";"      MAIN_MENU
keymap g?       HELP
keymap C-r      NEXT
keymap u        PREV
keymap H        PREV
